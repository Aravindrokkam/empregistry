<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <servlet>
    <servlet-name>SaveServlet</servlet-name>
    <servlet-class>com.controller.emp.SaveServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>SaveServlet</servlet-name>
    <url-pattern>/SaveServlet</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>ViewServlet</servlet-name>
    <servlet-class>com.controller.emp.ViewServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ViewServlet</servlet-name>
    <url-pattern>/ViewServlet</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>EditServlet</servlet-name>
    <servlet-class>com.controller.emp.EditServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>EditServlet</servlet-name>
    <url-pattern>/EditServlet</url-pattern>
  </servlet-mapping>
  
    
   <servlet>
    <servlet-name>EditServlet2</servlet-name>
    <servlet-class>com.controller.emp.EditServlet2</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>EditServlet2</servlet-name>
    <url-pattern>/EditServlet2</url-pattern>
  </servlet-mapping>
</web-app>
